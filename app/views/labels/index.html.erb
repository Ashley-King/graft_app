<p id="notice"><%= notice %></p>
<div class="container">
<p id="notice"><%= notice %></p>
  <h1>Labels</h1>

<table class="table table-striped">
  <thead>
    <tr>
      <th>Wine year</th>
      <th>Wine name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @labels.each do |label| %>
      <tr>
        <td><%= label.wine_year %></td>
        <td><%= label.wine_name %></td>
        
        
        <td><%= link_to 'Show', label %></td>
        <td><%= link_to 'Edit', edit_label_path(label) %></td>
        <td><%= link_to 'Print Label', print_label_path(label) %></td>
        <td><%= link_to 'Destroy', label, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Label', new_label_path %>
</div>

